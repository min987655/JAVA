package ch02;

public class VarEx03 {

	public static void main(String[] args) {
		char s1 = '가';//2Byte
		System.out.println(s1);
		
		s1 = '나';//s1의 값 변경
		System.out.println(s1);
		
		char c1 = '가';
		char c2 = '나';
		char c3 = '다';
		System.out.print(c1);
		System.out.print(c2);
		System.out.print(c3);
	}

}
